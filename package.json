{
  "name": "wp-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "wp",
    "build:dev": "wp --mode development --compi 1",
    "start": "wp --mode development",
    "build:none": "wp --mode none",
    "build:legacy": "wp --mode prod:legacy",
    "build:modern": "wp --mode prod:modern",
    "watch": "nodemon --watch webpack.*.js --exec \"npm start\"",
    "build:stats": "wp --mode production --json > stats.json",
    "build:prod-analyzer": "wp --mode production --analyzeri 1",
    "build:multi": "wp --config webpack.multi.js",
    "build:ssr": "wp --config webpack.ssr.js",
    "build:mf": "wp --config webpack.mf.js --mode production",
    "start:mf": "wp --config webpack.mf.js --mode development",
    "build:mf-header": "npm run build:mf -- --component header",
    "start:mf-app": "npm run start:mf -- --component app",
    "test:mocha": "mocha tests",
    "test:mocha:watch": "wp --config webpack.mocha.js",
    "deploy:gh-pages": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "assert": "^1.5.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.10.0",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "gh-pages": "^3.1.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "glob": "^7.1.6",
    "loader-runner": "^4.2.0",
    "mini-css-extract-plugin": "^1.4.0",
    "mini-html-webpack-plugin": "^3.1.3",
    "mocha": "^8.3.2",
    "mocha-loader": "^5.1.5",
    "nodemon": "^2.0.7",
    "postcss-loader": "^5.2.0",
    "purgecss-webpack-plugin": "^4.0.3",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.4",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.3.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-merge": "^5.7.3",
    "webpack-nano": "^1.1.1",
    "webpack-plugin-serve": "^1.3.0"
  }
}
